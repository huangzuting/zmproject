@import "product-item/product-item-template.wxss";

swiper {
  width: 100%;
  height: 600rpx;
}

/*less sass*/

swiper image {
  width: 100%;
  height: 600rpx;
}

.icon {
  width: 9px;
  height: 16px;
  margin-left: 20rpx;
  margin-top: 9rpx;
}

.section {
  flex-direction: column;
  display: flex;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 10rpx;
  margin-right: 100rpx;
  background-color: #fff;
  padding-bottom: 10rpx;
  width: 100%;
}

.checkbox {
  flex-direction: row;
  display: flex;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 10rpx;
  background-color: #fff;
  padding-bottom: 5px;
}

.text{
    font-size:32rpx;
    font-weight: bold;
}